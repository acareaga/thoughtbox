<div class="container">
  <h3>Login</h3>
  <br><br>
  <%= form_for(:session, url: login_path) do |f| %>
    <%= f.label :email_address %><br>
    <%= f.text_field :email_address %><br>
    <%= f.label :password %><br>
    <%= f.password_field :password %><br>
    <%= f.button "Login" %>
  <% end %>
</div>
